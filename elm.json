{
    "type": "package",
    "name": "hecrj/composable-form",
    "summary": "Build type-safe composable forms in Elm",
    "license": "BSD-3-Clause",
    "version": "4.0.0",
    "exposed-modules": [
        "Form",
        "Form.Value",
        "Form.View",
        "Form.Error",
        "Form.Field",
        "Form.Base",
        "Form.Base.TextField",
        "Form.Base.NumberField",
        "Form.Base.RangeField",
        "Form.Base.CheckboxField",
        "Form.Base.RadioField",
        "Form.Base.SelectField"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.0 <= v < 2.0.0",
        "elm/html": "1.0.0 <= v < 2.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.1.0 <= v < 2.0.0"
    }
}
