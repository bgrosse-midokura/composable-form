{
    "version": "2.2.0",
    "summary": "Build type-safe composable forms in Elm",
    "repository": "https://github.com/hecrj/composable-form.git",
    "license": "BSD3",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Form",
        "Form.Value",
        "Form.View",
        "Form.Error",
        "Form.Field",
        "Form.Base",
        "Form.Base.TextField",
        "Form.Base.NumberField",
        "Form.Base.RangeField",
        "Form.Base.CheckboxField",
        "Form.Base.RadioField",
        "Form.Base.SelectField"
    ],
    "dependencies": {
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
